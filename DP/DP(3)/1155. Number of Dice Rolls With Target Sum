1155. Number of Dice Rolls With Target Sum
class Solution {
public:
int solveRec(int n,int k,int target)
{    //base case
    if(target<0 || n<0)
    {
    return 0;
    }
    if(n==0 && target==0)
    { //ek tareeka target ka 
      return 1;
    }
    if(n==0 && target!=0)
    {  //non zero targt
        //but dice nhi hai 
        return 0;
    }
    if(n!=0 && target==0)
    {
        return 0;
    }
    //recursive logic
    //n no: ka sum krna hai
    //dice ke k faces hai
    int ans=0;
    for(int i=1;i<=k;i++)
    {
        ans+=solveRec(n-1,k,target-i);
    }
    return ans;
}
int solveMem(int n,int k,int target)
{    //base case
    if(target<0 || n<0)
    {
    return 0;
    }
    if(n==0 && target==0)
    { //ek tareeka target ka 
      return 1;
    }
    if(n==0 && target!=0)
    {  //non zero targt
        //but dice nhi hai 
        return 0;
    }
    if(n!=0 && target==0)
    {
        return 0;
    }
    //recursive logic
    //n no: ka sum krna hai
    //dice ke k faces hai
    int ans=0;
    for(int i=1;i<=k;i++)
    {
        ans+=solveRec(n-1,k,target-i);
    }
    return ans;
}


    int numRollsToTarget(int n, int k, int target) {
       // return solveRec(n,k,target);
       //2d dp
       vector<vector<int>>dp((n+1,-1));
    }
};
